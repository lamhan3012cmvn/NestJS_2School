{
  "name": "nestjs_microservice",
  "version": "0.0.1",
  "description": "nestjs microservice",
  "author": "o0oKenZyo0o",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build client && cp -r .env ./dist",
    "format": "prettier --write \"apps/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start client --watch",
    "start:dev": "yarn format &&./scripts/start.dev.sh",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.1",
    "@nestjs/common": "^8.0.2",
    "@nestjs/core": "^8.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.0.2",
    "@nestjs/mongoose": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.4",
    "@nestjs/platform-socket.io": "^8.0.9",
    "@nestjs/swagger": "^5.0.3",
    "@nestjs/terminus": "^7.2.0",
    "@nestjs/websockets": "^8.0.9",
    "bcrypt": "^5.0.1",
    "blurhash": "^1.1.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "express-rate-limit": "^5.3.0",
    "file-type": "^16.5.3",
    "helmet": "^4.6.0",
    "mongoose": "^5.13.2",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "sharp": "^0.29.1",
    "swagger-ui-express": "^4.1.6",
    "typegoose": "^5.9.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^8.0.4",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.36",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^9.2.3",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
